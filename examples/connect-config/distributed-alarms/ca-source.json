{
  "name" : "ca-source",
  "config" : {
    "connector.class" : "org.jlab.kafka.CASourceConnector",
    "tasks.max" : "3",
    "epics.ca.addr.list": "softioc",
    "channels.topic": "epics-channels",
    "channels.group": "ca-source",
    "bootstrap.servers": "kafka://kafka:9092",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://registry:8081",
    "transforms": "replaceField,renameField,toString,substitute",
    "transforms.replaceField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.replaceField.blacklist": "timestamp,status,floatValues,stringValues,intValues",
    "transforms.renameField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.renameField.renames": "severity:priority",
    "transforms.toString.type": "org.apache.kafka.connect.transforms.Cast$Value",
    "transforms.toString.spec": "priority:string",
    "transforms.substitute.type": "org.jlab.kafka.transforms.Substitute$Value",
    "transforms.substitute.field.name": "priority",
    "transforms.substitute.regex": "3",
    "transforms.substitute.replacement": "300"
  }
}